        <control progid="DecalControls.CheckBox" name="Enable" left="10" top="40" width="70" height="20" text="Enable" checked="True"/>
        <control progid="DecalControls.Slider" name="Volume" left="90" top="40" width="170" height="20" minimum="0" maximum="100" position="75" />

